    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-standard": "^11.0.0",

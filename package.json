{
  "name": "demo-web-client",
  "version": "1.0.0",
  "description": "This is a demo for developing an web client for all skills",
  "scripts": {
    "build": "lerna run build --scope web-server --stream",
    "check:lint": "lerna run check:lint",
    "lint:fix": "lerna run lint:fix",
    "clean": "lerna clean --yes && rimraf ./node_modules",
    "prelink-packages": "npm install",
    "link-packages": "lerna bootstrap",
    "format:dry": "lerna run format:dry",
    "format": "lerna run format",
    "start": "lerna run start --scope web-server --stream",
    "start:dev": "lerna run start:dev --scope web-server --stream",
    "pretest": "rimraf ./packages/*/tests/out",
    "test": "npm run check:lint && npm run test:unit",
    "test:unit": "lerna exec npm run test:unit --scope \"@(landing-web)\" --stream --concurrency 1",
    "pretest:e2e": "webdriver-manager update --ignore_ssl",
    "test:e2e": "protractor protractor.conf.js",
    "update:routes": "gulp update",
    "ready": "npm run link-packages && npm run update:routes"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run check:lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/NoroLinxy/demo-web-client.git"
  },
  "keywords": [
    "web",
    "client"
  ],
  "author": "noro",
  "license": "SEE LICENSE IN license",
  "bugs": {
    "url": "https://gitlab.com/NoroLinxy/demo-web-client/issues"
  },
  "homepage": "https://gitlab.com/NoroLinxy/demo-web-client/blob/master/README.md",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0"
  },
